alias ctags="`brew --prefix`/bin/ctags"
eval "$(hub alias -s)"

alias tmux='tmux -2'
alias ta='tmux attach -t'
alias tnew='tmux new -s'
alias tls='tmux ls'
alias tkill='tmux kill-session -t'

alias vim=nvim
alias ev='vim ~/.vimrc'
alias et='vim ~/.tmux.conf'
alias ex='vim ~/.zshrc'
tag () { vim -c "tag $1" }
alias vimf='nvim $(fzf)'
